# Copy this file to .env and update the values as needed

# Base path for the application (for deployment)
BASENAME=/

# Vite environment variables (prefix with VITE_ to expose to client)
VITE_APP_NAME=StarWars Blog
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://www.swapi.tech/api

# Development settings
NODE_ENV=development

# Optional: Custom dev server settings
# VITE_DEV_SERVER_HOST=localhost
# VITE_DEV_SERVER_PORT=3000

# Optional: For production builds
# VITE_BUILD_SOURCEMAP=true